<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('/images/bg-01.jpg');">
            <div class="wrap-login100" style="background:#FFDEAD; width:1500px">
                <form action="@ViewData["ReturnUrl"]" method="post">
                    <span class="login100-form-title p-b-34 p-t-27"  style="color:black;">
                        SECTION C
                    </span>
                    <label asp></label>
                    <div asp-validation-summary="All" class="text-danger"></div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I keep a watching eye on areas where difficulty may arise
                        </label>
                        <input asp-for="Shaper3" class="input100" type="text" placeholder="">
                        <span class="focus-input100" ></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I explore ideas that may have a wider application than in the immediate task
                        </label>
                        <input asp-for="Coordinator3" class="input100" type="text" placeholder="">
                        <span class="focus-input100" ></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I like to weigh up and evaluate a range of suggestions thoroughly before choosing
                        </label>
                        <input asp-for="Planter3" class="input100" type="text" placeholder="">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I can co-ordinate and use productively other people's abilities and talents
                        </label>
                        <input asp-for="Resource3" class="input100" type="text" placeholder="">
                        <span class="focus-input100" data-placeername"holder="&#xf207;"></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I maintain a steady systematic approach, whatever the pressure
                        </label>
                        <input asp-for="Monitor3" class="input100" type="text" placeholder="">
                        <span class="focus-input100" ></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I often produce a new approach to a long continuing problem
                        </label>
                        <input asp-for="Implementer3" class="input100" type="text" placeholder="">
                        <span class="focus-input100" ></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I am ready to make my personal views known in a forceful way if necessary
                        </label>
                        <input asp-for="Teamwork3" class="input100" type="text" placeholder="">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input">
                        <label>
                            I am ready to help whenever I can
                        </label>
                        <input asp-for="Finisher3" class="input100" type="text" placeholder="">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn" style="background:#FFDEAD">
                            NEXT
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div id="dropDownSelect1"></div>
    <!--===============================================================================================-->
    <script src="~/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/bootstrap/js/popper.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>

</body>